{
  "app": {
    "name": "Epilepsy_Health_App HealthData Service",
    "service": "health-data-service",
    "version": "1"
  },
  "swagger": {
    "title": "Epilepsy_Health_App.Services.HealthData",
    "version": "v1",
    "contact": "filap4@gmail.com"
  },
  "logger": {
    "level": "information",
    "excludePaths": [ "/ping", "/metrics" ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://seq:5341",
      "apiKey": "secret"
    },
    "graylog": {
      "enabled": true,
      "url": "localhost",
      "port": "12201",
      "transportType": "Udp"
    },
    "efk": {
      "enabled": true,
      "url": "localhost",
      "port": 24224
    },
    "AllowedHosts": "*"
  }
}
